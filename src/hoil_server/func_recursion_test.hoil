define fibonacci(num) -> Number
{
    if num is 1
    {
        return 1
    }
    elif num <= 0
    {
        return 0
    }

    return fibonacci(num - 2) + fibonacci(num - 1)
}

result is fibonacci(8)
call Print("8th fibonacci number is {result}")

